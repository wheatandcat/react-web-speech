{
  "name": "react-web-speech",
  "version": "0.5.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "parcel ./example/index.html",
    "example":
      "NODE_ENV=production parcel build ./example/index.html -d ./examples-dist --no-cache --no-minify",
    "build": "run-s build:*",
    "build:init": "rm -rf dist && mkdir dist",
    "build:js": "NODE_ENV=production babel src --out-dir dist",
    "build:install":
      "cp package.json dist/ && cd dist && yarn install --production",
    "build:chmod": "chmod -R 777 dist/*.js",
    "storybook": "start-storybook -p 9009",
    "build-storybook": "build-storybook",
    "deploy-storybook": "storybook-to-ghpages"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wheatandcat/react-web-speech.git"
  },
  "author": "wheatandcat",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wheatandcat/react-web-speech/issues"
  },
  "homepage": "https://github.com/wheatandcat/react-web-speech#readme",
  "dependencies": {
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "styled-components": "^3.1.6"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.3.11",
    "@storybook/addon-knobs": "^3.3.11",
    "@storybook/addon-options": "^3.3.11",
    "@storybook/cli": "^3.3.11",
    "@storybook/react": "^3.3.12",
    "@storybook/storybook-deployer": "^2.2.0",
    "babel-cli": "^6.26.0",
    "babel-preset-react-app": "^3.1.1",
    "npm-run-all": "^4.1.2",
    "parcel-bundler": "^1.5.1",
    "prettier": "^1.10.2"
  }
}
